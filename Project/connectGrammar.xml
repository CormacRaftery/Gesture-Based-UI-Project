<?xml version="1.0" encoding="utf-8" ?>
<grammar version="1.0" xml:lang="en-us"
         mode="voice"
         xmlns="http://www.w3.org/2001/06/grammar"
         tag-format="semantics/1.0"
         root="gameoptions">

  <rule id="gameoptions">
    <one-of>
      <item>
        <tag>out.command="0";</tag>
        <one-of>
          <item>0</item>
          <item>zero</item>
          <item>row zero</item>
          <item>row 0</item>
          <item>nought</item>
          <item>row nought</item>
          <item>column zero</item>
          <item>column won</item>
          <item>column 0</item>
        </one-of>
      </item>
      <item>
        <tag>out.command="1";</tag>
        <one-of>
          <item>1</item>
          <item>one</item>
          <item>won</item>
          <item>first</item>
          <item>row 1</item>
          <item>row one</item>
          <item>row won</item>
          <item>first row</item>
          <item>column 1</item>
          <item>column one</item>
          <item>column won</item>
          <item>first column</item>
        </one-of>
      </item>
      <item>
        <tag>out.command="2";</tag>
        <one-of>
          <item>2</item>
          <item>too</item>
          <item>to</item>
          <item>two</item>
          <item>second</item>
          <item>column 2</item>
          <item>column too</item>
          <item>column to</item>
          <item>column two</item>
          <item>second column</item>
          <item>row 2</item>
          <item>row too</item>
          <item>row to</item>
          <item>row two</item>
          <item>second row</item>
        </one-of>
      </item>
      <item>
        <tag>out.command="3";</tag>
        <one-of>
          <item>3</item>
          <item>tree</item>
          <item>three</item>
          <item>third</item>
          <item>row 3</item>
          <item>row tree</item>
          <item>row three</item>
          <item>third row</item>
          <item>column 3</item>
          <item>column tree</item>
          <item>column three</item>
          <item>third column</item>
        </one-of>
      </item>
      <item>
        <tag>out.command="4";</tag>
        <one-of>
          <item>4</item>
          <item>fourth</item>
          <item>row 4</item>
          <item>fourth row</item>
          <item>column 4</item>
          <item>fourth column</item>
        </one-of>
      </item>
      <item>
        <tag>out.command="5";</tag>
        <one-of>
          <item>5</item>
          <item>fifth</item>
          <item>row 5</item>
          <item>fifth row</item>
          <item>column 5</item>
          <item>fifth column</item>
        </one-of>
      </item>
      <item>
        <tag>out.command="6";</tag>
        <one-of>
          <item>6</item>
          <item>sixth</item>
          <item>row 6</item>
          <item>sixth row</item>
          <item>column 6</item>
          <item>sixth column</item>
        </one-of>
      </item>
    </one-of>
  </rule>
</grammar>


